pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Harshit is Building the project...'
                sh '''mkdir harry
                      cd harry
                      touch name.txt'''
            }
        }
        stage('Test') {
            steps {
                echo 'Testing the project...'
                sh 'cat build/status.txt'
            }
        }
    }
}
